#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typedoc@0.28.13_typescript@5.9.2/node_modules/typedoc/bin/typedoc" "$@"
else
  exec node  "$basedir/../../../../../../typedoc@0.28.13_typescript@5.9.2/node_modules/typedoc/bin/typedoc" "$@"
fi
